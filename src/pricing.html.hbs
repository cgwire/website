  <script>
  function hideByClassName (className) {
    var els = document.getElementsByClassName(className)
    for (i = 0 ; i < els.length ; i++) {
      els[i].className += ' hidden'
    }
  }

  function showByClassName (className) {
    var els = document.getElementsByClassName(className)
    for (i = 0 ; i < els.length ; i++) {
      els[i].className = els[i].className.replace(' hidden', '')
    }
  }

  var monthlyButton = document.getElementById('monthly-button')
  var yearlyButton = document.getElementById('yearly-button')
  monthlyButton.addEventListener('click', function () {
    if (monthlyButton.className.indexOf('active') < 0) {
      hideByClassName('yearly')
      hideByClassName('pricing-price-info')
      showByClassName('monthly')
      monthlyButton.className += ' active'
      yearlyButton.className = yearlyButton.className.replace(' active', '')
    }
  })
  yearlyButton.addEventListener('click', function () {
    if (yearlyButton.className.indexOf('active') < 0) {
      hideByClassName('monthly')
      showByClassName('pricing-price-info')
      showByClassName('yearly')
      yearlyButton.className += ' active'
      monthlyButton.className = monthlyButton.className.replace(' active', '')
    }
  })
